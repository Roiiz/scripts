task.wait(10)
local hasToken = nil
for i,v in pairs(game:GetService("Players").LocalPlayer.MemoryCard.Tokens:GetChildren()) do
  if v.Name == Dino then
    hasToken = true
    game:GetService("TeleportService"):Teleport(615881305, game.Players.LocalPlayer)
		task.wait(0.3)
	for i=1, 999 do
		spawn(function() print(workspace.GameFunctions.ConvertToken:InvokeServer(Dino)) end)
	end
  end
end
if not hasToken then
  for i=1, 10 do
	task.wait(1.1)
		spawn(function() print(workspace.GameFunctions.ConvertSkin:InvokeServer(Dino)) end)
	end
	game:GetService("TeleportService"):Teleport(615881305, game.Players.LocalPlayer)
end
